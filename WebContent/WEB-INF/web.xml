  	<servlet-name>AddDream</servlet-name>
  	<url-pattern>/addDream</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>Home</servlet-name>
  	<servlet-class>com.controller.Home</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>Delete</servlet-name>
  	<servlet-class>com.controller.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
 
  
</web-app>